{% extends 'base.html.twig' %}

{% block title %}Ajouter une fonction{% endblock %}

{% block body %}
    <div class="container-fluid">
        <div class="row">
            <div class="col s12 m12 l12">
                <h4>Ajout d'une fonction</h4>
            </div>
        </div>

  
        {{ form_start(form)}}  

        {{ form_label(form.code)}}
        {{ form_widget(form.code)}}       

        {{ form_label(form.LibelleFonction)}}
        {{ form_widget(form.LibelleFonction)}}
        {{ form_label(form.Ajouter)}}
        {{ form_widget(form.Ajouter)}}


        {{ form_end(form)}}

        <a href='/Admin'>Retour</a>
    </div>

{% endblock %}
